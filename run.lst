 461  exp0(i)    =max(0,sam(i,"export"));
 462  expf0(f)   =max(0,sam(f,"export"));
 463   
 464  xexp0(i)    =min(0,sam(i,"export"));
 465  xexpf0(f)   =min(0,sam(f,"export"));
 466   
 467  imp0(i)    =max(0,-sam(i,"import"));
