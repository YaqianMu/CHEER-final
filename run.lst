 457   
 458  xcons0(i)    =min(0,sam(i,"Household"))+min(0,sam(i,"GOVERNMENT"));
 459  xconsf0(f)   =min(0,sam(f,"Household"))+min(0,sam(f,"GOVERNMENT"));
